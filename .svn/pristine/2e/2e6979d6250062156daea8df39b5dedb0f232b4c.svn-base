.m_usercenter{
    width: 100%;

    background: #f0f8ff;
}
.m_user_ctop{
    text-align: center;
    background: url("../image/user_center/m_user_ctop.png");
    background-size: cover;
    padding-top: 1em;


}
.m_user_tx{
    padding-top: 1em;

    position: relative;

}
.m_user_via{
   text-align: center;
    margin: 0 auto;
    width: 98px;
    height: 98px;
    border-radius: 49px;
    border: 2px solid #ffffff;
        overflow: hidden;
}
.m_user_via img{

    display: block;
    width: 100%;
}
.m_user_login_box{
    overflow: auto;

    padding-bottom: 1em;
}
.m_user_login_box input{
    display: inline-block;
    background: none;
    border: 1px solid #ffffff;
    padding: .5em 1.5em;
    color: #ffffff;
}
.m_user_login{
    width: 50%;
    float: left;
    text-align: right;
}
.m_user_login input{
    margin-right: 1em;
}
.m_user_register{
    width: 50%;
    float: left;
    text-align: left;
}
.m_user_register input{
    margin-left: 1em;
}
.m_user_rank,.m_user_price{
    line-height: 3em;
    font-size: 12px;
    color: #fbfbaf;
    letter-spacing: .05em;
}
.m_user_rank{
    padding-right: .5em;
}
.m_user_price{
    padding-left: .5em;
}
.m_user_name{
    color: #ffffff;
    letter-spacing: .05em;

}
