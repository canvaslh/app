.m_register {

	width: 100%;
	height: 100%;
	background: #ffffff;
}

.m_register_topbg {}

.m_register_topbg img {
	width: 100%;
}

.m_register_box {
	height: 2.5em !important;
	line-height: 2.5em;
}

.m_register_form form ul li {
	border: 1px solid #d6d6d6;
	border-radius: 2em;
	margin-top: 2em;
}

.m_register_form form ul li:hover {
	border: 1px solid #1d80e6;
}

.m_register_yzmbox {
	/*border: none !important;*/
	position: relative;
	overflow: hidden;
}

.m_register_yzmbox_ {}

.m_register_yzmbox ul {
	overflow: auto;
	width: 100%;
}

.m_register_yzmbox ul li {
	display: block;
	margin-top: 0 !important;
}

.m_register_yzmbox ul li:nth-child(1) {
	width: 60%;
	float: left;
}

.m_register_yzmbox ul li:nth-child(2) {
	width: 30%;
	float: right;
	border: 1px solid #1d80e6;
}

.m_register_yzmbox ul li {}

.m_register_yzmbox ul li .m_login_pwd {
	width: 100%;
}

.m_register_yzmbox ul li .m_login_pwd label {
	float: left;
	width: 80%;
}

.m_register_yzmbox ul li input {
	width: 100%;
}

.m_register_yz {
	font-size: 9pt;
	position: absolute;
	right: 0;
	width: 35%;
	top: 0;
	bottom: 0;
	height: 2.8em!important;
	color: #9c9c9c;
	background: #ffffff;
	cursor: pointer;
}

input.m_register_yz[placeholder] {
	font-size: 12px;
	color: #000 !important;
}

input.m_register_yz::-webkit-input-placeholder {
	color: #000 !important;
}

.m_register_02 {
	line-height: 3em;
	margin-top: 2em;
	text-align: center;
	color: #4197fe;
	letter-spacing: .05em;
}

.m_register_login {
	font-size: 14px;
}

.m_register_items {
	line-height: 3em;
	margin-top: 2em;
	text-align: center;
	color: #9c9c9c;
}

.m_register_items input {
	vertical-align: middle;
	font-size: 12px;
}

.m_register_btn {
	margin-top: 1em;
	width: 80%;
	margin-left: auto;
	margin-right: auto;
	display: block;
	background: #439cfa;
	border: none;
	border-radius: 2em;
	height: 3em;
	line-height: 3em;
	color: #ffffff;
	text-align: center;
	box-shadow: 0 5px 4px #bcdaff;
	cursor: pointer;
}

.m_registeruser_icon {
	display: block;
	float: left;
	width: 20px;
	height: 20px;
	padding-top: 1em;
	background: url("../image/login/user_name.png") center center no-repeat;
	background-size: 14px 14px;
}

.m_register_form form ul li:hover .m_registeruser_icon {
	background: url("../image/login/username1.png") center center no-repeat;
	background-size: 14px 14px;
}

.m_registerhp_icon {
	display: block;
	float: left;
	width: 20px;
	height: 20px;
	padding-top: 1em;
	background: url("../image/login/dh_1.png") center center no-repeat;
	background-size: 14px 14px;
}

.m_register_form form ul li:hover .m_registerhp_icon {
	background: url("../image/login/dh.png") center center no-repeat;
	background-size: 14px 14px;
}

.m_registerpwd_icon {
	display: block;
	float: left;
	width: 20px;
	height: 20px;
	padding-top: 1em;
	background: url("../image/login/pwd.png") center center no-repeat;
	background-size: 14px 14px;
}

.m_register_form form ul li:hover .m_registerpwd_icon {
	background: url("../image/login/pwd1.png") center center no-repeat;
	background-size: 14px 14px;
}

.m_registeryzm_icon {
	display: block;
	float: left;
	width: 20px;
	height: 20px;
	padding-top: 1em;
	background: url("../image/login/yzm.png") center center no-repeat;
	background-size: 14px 14px;
}

.m_register_form form ul li:hover .m_registeryzm_icon {
	background: url("../image/login/yzm_1.png") center center no-repeat;
	background-size: 14px 14px;
}

.layui-layer-btn .layui-layer-btn0 {
	border: 1px solid #dedede;
	background-color: #f1f1f1;
	color: #fff
}